            <main class="main">

                <div class="container">
                    <h1 style="color: #c66;" style="background-color:black;" class="page-title"><span></span></h1>
                </div><!-- End .container -->

                <nav aria-label="breadcrumb" class="breadcrumb-nav mb-2">

                </nav><!-- End .breadcrumb-nav -->
            </main>






            <main class="main" style="margin-top: 100px;">
                <nav aria-label="breadcrumb" class="breadcrumb-nav border-0 mb-0">
                    <div class="container d-flex align-items-center">
                        <ol class="breadcrumb">
                            <li class="breadcrumb-item"><a href="/">Home</a></li>
                            <li class="breadcrumb-item"><a href="/product-list">Shop</a></li>
                            <li class="breadcrumb-item active" aria-current="page">
                                <%= product.productName %>
                            </li>
                        </ol>


                    </div><!-- End .container -->
                </nav><!-- End .breadcrumb-nav -->

                <div class="page-content">
                    <div class="container">
                        <div class="product-details-top mb-2">
                            <div class="row">
                                <div class="col-md-6">
                                    <div class="product-gallery product-gallery-vertical">
                                        <div class="row">
                                            <figure class="product-main-image">
                                                <img id="product-zoom" src="/images/<%=product.images[0]%>"
                                                    data-zoom-image="/images/<%=product.images[0]%>">

                                                <a href="#" id="btn-product-gallery" class="btn-product-gallery">
                                                    <i class="icon-arrows"></i>
                                                </a>
                                            </figure><!-- End .product-main-image -->
                                           
                                                   
                                            <style>
                                                .zoom {
                                                    padding: 78px;

                                                    transition: transform .2s;
                                                    width: 400px;
                                                    height: 400px;
                                                    margin: 0 auto;
                                                }

                                                .zoom:hover {
                                                    -ms-transform: scale(1.5);
                                                    /* IE 9 */
                                                    -webkit-transform: scale(1.5);
                                                    /* Safari 3-8 */
                                                    transform: scale(1.5);
                                                }
     




                                            </style>

                                            <div id="product-zoom-gallery" class="product-image-gallery">
                                                <a class="product-gallery-item active" href="#"
                                                    data-image="/images/<%=product.images[0]%>"
                                                    data-zoom-image="/images/<%=product.images[0]%>">
                                                    <img src="/images/<%=product.images[0]%>" alt="product side">
                                                </a>

                                                <a class="product-gallery-item" href="#"
                                                    data-image="/images/<%=product.images[1]%>"
                                                    data-zoom-image="/images/<%=product.images[1]%>">
                                                    <img src="/images/<%=product.images[1]%>" alt="product cross">
                                                </a>

                                                <a class="product-gallery-item" href="#"
                                                    data-image="/images/<%=product.images[2]%>"
                                                    data-zoom-image="/images/<%=product.images[2]%>">
                                                    <img src="/images/<%=product.images[2]%>" alt="product with model">
                                                </a>


                                            </div><!-- End .product-image-gallery -->
                                        </div><!-- End .row -->
                                    </div><!-- End .product-gallery -->
                                </div><!-- End .col-md-6 -->

                                <div class="col-md-6">
                                    <div class="product-details product-details-centered">
                                        <h1 class="product-title">
                                            <%= product.productName %>
                                        </h1><!-- End .product-title -->

                                        <div class="product-price">


                                            <!-- Regular Price -->
                                            <p class="regular-price">Rs.<%= product.Price %>
                                            </p>

                                        </div><!-- End .product-price -->

                                        <!-- Other details... -->

                                        <div class="product-details-action">

                                            <div class="product-details-action">
                                                <div class="details-action-col">
                                                    <% if (product.stock> 0) { %>
                                                        <a href="/add-to-cart/<%= product._id %>"
                                                            class="btn-product btn-cart"><span>add to cart</span></a>
                                                        <% } else { %>
                                                            <span class="btn-product btn-out-of-stock"><span>Out of
                                                                    Stock</span></span>
                                                            <% } %>
                                                </div><!-- End .details-action-col -->

                                                <div class="details-action-wrapper">
                                                    <a href="/addwishlist/<%= product._id %>"
                                                        class="btn-product btn-wishlist" title="Wishlist"><span>Add to
                                                            Wishlist</span></a>
                                                </div><!-- End .details-action-wrapper -->
                                            </div><!-- End .product-details-action -->

                                            <!-- Other details... -->

                                        </div><!-- End .product-details -->

                                    </div><!-- End .product-details-action -->

                                    <!-- Other details... -->

                                </div><!-- End .product-details -->
                            </div><!-- End .col-md-6 -->

                        </div><!-- End .row -->
                    </div><!-- End .product-details-top -->

